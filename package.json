{
  "name": "eslint-config-allanchau",
  "version": "9.1.0",
  "description": "Comprehensive ESLint config.",
  "keywords": [
    "eslint",
    "config",
    "allanchau"
  ],
  "files": [
    "src"
  ],
  "author": "Allan Chau",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allanchau/node-eslint-config-allanchau"
  },
  "bugs": {
    "url": "https://github.com/allanchau/node-eslint-config-allanchau/issues"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.7"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0"
  }
}
