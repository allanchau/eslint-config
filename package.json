{
  "name": "eslint-config-allanchau",
  "version": "9.1.1",
  "description": "Comprehensive ESLint config.",
  "keywords": [
    "eslint",
    "config",
    "allanchau"
  ],
  "files": [
    "src"
  ],
  "author": "Allan Chau",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allanchau/node-eslint-config-allanchau"
  },
  "bugs": {
    "url": "https://github.com/allanchau/node-eslint-config-allanchau/issues"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ],
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "dependencies": {
    "babel-eslint": "10.0.2",
    "confusing-browser-globals": "1.0.8",
    "eslint": "6.1.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsdoc": "15.8.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "1.7.0"
  }
}
