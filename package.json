{
  "name": "@allanchau/eslint-config",
  "version": "7.0.2",
  "description": "Comprehensive ESLint config.",
  "keywords": [
    "eslint",
    "config",
    "allanchau"
  ],
  "files": [
    "src"
  ],
  "author": "Allan Chau",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/allanchau/node-eslint-config-allanchau"
  },
  "bugs": {
    "url": "https://github.com/allanchau/node-eslint-config-allanchau/issues"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "dependencies": {
    "eslint": "^5.8.0",
    "eslint-plugin-eslint-comments": "^3.0.1"
  }
}
